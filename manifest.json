{
  "manifest_version": 3,
  "name": "AI Form Change",
  "version": "1.0",
  "description": "AI-powered form modification extension",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_title": "AI Form Change"
  }
}